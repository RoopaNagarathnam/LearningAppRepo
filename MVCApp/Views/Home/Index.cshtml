<!-- Views/Employee/Details.cshtml -->
@model List<Employee>

@{
    ViewData["Title"] = "Employee Details";
}

<h1>@ViewData["Title"]</h1>

<div width="1000px">
    <table width="1000px" border="1" style="background-color:aqua">
    <tr>
        <th>
            Employee ID
        </th>
        <th>
            Employee Name
        </th>
        <th>
            Employee Designation
        </th>
    </tr>
    
    @foreach (var employee in Model)
    {
       <tr>

            <td>@employee.EmployeeId</td>
            <td>@employee.EmployeeName</td>
            <td>@employee.EmployeeDesignation</td>
       </tr>
    }
    </table>

</div>

<a href="@Url.Action("Index")">Back to List</a>
